{
  "name": "kalios-rest",
  "version": "0.0.0",
  "description": "Rest API for Kalios project",
  "main": "server.js",
  "scripts": {
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha tests/**/*.test.js",
    "test:watch": "nodemon --exec \" npm i && npm test\"",
    "start": "npm run lint && node server.js",
    "dev": "./node_modules/.bin/nodemon --exec \"npm i && npm start\"",
    "lint": "eslint .",
    "lint:fix": "eslint **/*.js --fix && eslint server.js --fix"
  },
  "author": "Kalios Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.13.0",
    "mongodb": "^2.2.31",
    "mongoose": "^5.2.4",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "supertest": "^3.0.0"
  }
}
